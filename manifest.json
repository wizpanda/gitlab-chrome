{
    "name": "GitLab Improve MR",
    "version": "1.0.0",
    "description": "Improve visibility of Merge Request pages of GitLab",
    "permissions": [
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://gitlab.com/*/merge_requests/*"
            ],
            "js": [
                "jquery-3.3.1.min.js",
                "improve-page.js"
            ],
            "css": [
                "improve-page.css"
            ]
        }
    ]
}
